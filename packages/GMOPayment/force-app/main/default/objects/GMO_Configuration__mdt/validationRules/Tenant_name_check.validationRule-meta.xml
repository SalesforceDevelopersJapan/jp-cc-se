<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Tenant_name_check</fullName>
    <active>true</active>
    <errorConditionFormula>NOT( REGEX( tdTenantName__c , &quot;^[a-zA-Z0-9]+$&quot;))</errorConditionFormula>
    <errorMessage>Please enter half-width alphanumeric characters excluding special characters.</errorMessage>
</ValidationRule>
